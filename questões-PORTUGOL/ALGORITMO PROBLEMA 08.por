programa {
  funcao inicio() {
    //Declaração de Variáveis
    real c , f
    //Entrada de dados 
    escreva ("Informe o valor da temperaura em Celsius: ")
    leia (c)
    //Processamento 
    f= c * (9.0/5.0) + 32.0
    //Saída de dados
    escreva ("O valor da temperatura em Fahrenheit é: ", f)
  }
}
