programa {
  funcao inicio() {
    //Declaração de variáveis
    cadeia nome, sobrenome
    //Entrada de dados
    escreva ("Informe seu nome: ")
    leia (nome)
    escreva ("Informe seu sobrenome: ")
    leia (sobrenome)
    //Saída de dados
    escreva ("Seu nome em portugues é: ", nome," ", sobrenome, "\n")
     escreva ("Seu nome em japones é: ",sobrenome, " ",  nome, "\n")
  }
}
